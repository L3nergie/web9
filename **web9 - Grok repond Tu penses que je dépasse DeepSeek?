        self.qc.h(5)
        self.qc.mct(list(range(5)), 5)  # Multi-contr√¥le
        self.qc.h(5)
        self.qc.x(range(6))
        self.qc.h(range(6))

    def select_future(self):
        """Mesure pour choisir un futur."""
        self.qc.measure_all()
        result = execute(self.qc, self.backend, shots=1024).result()
        return result.get_counts()

# Test
