        self.qc = QuantumCircuit(num_qubits)
        self.backend = Aer.get_backend('qasm_simulator')

    def explore_futures(self):
        """Cr√©e une superposition pour repr√©senter des futurs possibles."""
        self.qc.h(range(6))  # Superposition compl√®te
