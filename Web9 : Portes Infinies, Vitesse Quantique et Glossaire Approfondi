import json

def charger_glossaire():
    with open("web9_glossaire.json", "r") as f:
        return json.load(f)

def consulter_glossaire(terme: str):
    glossaire = charger_glossaire()
    info = glossaire.get(terme, {"nom": "Inconnu", "symbolisme": "Non d√©fini"})
